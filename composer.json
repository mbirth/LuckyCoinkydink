{
    "repositories": [
        {
            "type": "composer",
            "url": "https://packages.zendframework.com/"
        }
    ],
    "require": {
        "php": "~7.0",
        "katzgrau/klogger": "1.0.*",
        "voku/simple-cache": "4.*"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "apcu-autoloader": true,
        "optimize-autoloader": true,
        "platform": {
            "php": "7.99.99"
        },
        "sort-packages": true
    }
}
